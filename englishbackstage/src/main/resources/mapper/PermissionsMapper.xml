<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="sicnu.sixteam.englishbackstage.mapper.PermissionsMapper">
    <select id="getPermissionsByPhone" resultType="java.lang.String">
        SELECT permisname FROM permissions_id where permisid in (select permisid from permission_role where phone=#{phonenum});
    </select>
</mapper>
